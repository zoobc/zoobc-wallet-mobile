{
  "name": "spinechainwallet",
  "version": "0.0.1",
  "author": "BlockchainZoo Wallet Team",
  "homepage": "https://blockchainzoo.com/",
  "scripts": {
    "postinstall": "git submodule update --init --recursive && cd externals/bip32 && npm install --ignore-scripts",
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:ios": "cordova build ios --buildFlag='-UseModernBuildSystem=0'",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "grpc:build": "protoc --plugin=\"protoc-gen-ts='node_modules/.bin/protoc-gen-ts'\" --grpc-web_out=\"import_style=typescript,mode=grpcwebtext:src/grpc/generated\" --js_out=\"import_style=commonjs,binary:src/grpc/generated\" src/grpc/schema/service/*.proto --proto_path=src/grpc/schema && protoc --plugin=\"protoc-gen-ts='node_modules/.bin/protoc-gen-ts'\" --grpc-web_out=\"import_style=typescript,mode=grpcwebtext:src/grpc/generated\" --js_out=\"import_style=commonjs,binary:src/grpc/generated\" src/grpc/schema/model/*.proto --proto_path=src/grpc/schema",
    "s1": "protoc --proto_path=./src/app/grpc ./src/app/grpc/service/service.proto --js_out=import_style=commonjs:./src/app/grpc --grpc-web_out=import_style=commonjs,mode=grpcweb:./src/app/grpc",
    "s1s": "protoc --proto_path=./src/app/grpc ./src/app/grpc/service/service.proto --js_out=import_style=typescript:./src/app/grpc --grpc-web_out=import_style=typescript,mode=grpcweb:./src/app/grpc",
    "c1": "protoc --proto_path=./src/app/grpc ./src/app/grpc/model/model.proto --js_out=import_style=commonjs:./src/app/grpc --grpc-web_out=import_style=commonjs,mode=grpcweb:./src/app/grpc",
    "c1s": "protoc --proto_path=./src/app/grpc ./src/app/grpc/model/model.proto --js_out=import_style=typescript:./src/app/grpc --grpc-web_out=import_style=typescript,mode=grpcweb:./src/app/grpc"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^7.2.2",
    "@angular/core": "^7.2.2",
    "@angular/forms": "^7.2.2",
    "@angular/http": "^7.2.2",
    "@angular/platform-browser": "^7.2.2",
    "@angular/platform-browser-dynamic": "^7.2.2",
    "@angular/router": "^7.2.2",
    "@ionic-native/clipboard": "^5.8.0",
    "@ionic-native/core": "^5.10.0",
    "@ionic-native/network": "^5.10.0",
    "@ionic-native/qr-scanner": "^5.8.0",
    "@ionic-native/splash-screen": "^5.0.0",
    "@ionic-native/status-bar": "^5.0.0",
    "@ionic/angular": "^4.1.0",
    "@ionic/storage": "^2.2.0",
    "@keyhub/keyhub-vault-nxt": "^0.1.0",
    "@ngx-translate/core": "^11.0.1",
    "@ngx-translate/http-loader": "^4.0.0",
    "@ngxs/store": "^3.4.3",
    "cordova-android": "8.0.0",
    "cordova-clipboard": "1.2.1",
    "cordova-ios": "5.0.1",
    "cordova-plugin-network-information": "2.0.2",
    "cordova-plugin-qrscanner": "3.0.1",
    "core-js": "^2.5.4",
    "crypto-js": "^3.1.9-1",
    "diceware-wordlist-en-eff": "^1.0.1",
    "elliptic": "^6.4.1",
    "google-protobuf": "^3.8.0",
    "grpc-web": "^1.0.4",
    "jayson": "^3.0.2",
    "jsbi": "^2.0.5",
    "moment": "^2.24.0",
    "ngx-build-plus": "^7.8.3",
    "ngx-qrcode2": "0.0.9",
    "node-fetch": "^2.6.0",
    "node-sass": "^4.12.0",
    "qrcode-generator": "^1.4.3",
    "rxjs": "^6.5.2",
    "supercop.wasm": "^5.0.1",
    "ts-protoc-gen": "^0.10.0",
    "tslib": "^1.9.0",
    "tweetnacl": "^1.0.1",
    "zone.js": "~0.8.29"
  },
  "devDependencies": {
    "@angular-devkit/architect": "~0.12.3",
    "@angular-devkit/build-angular": "~0.12.3",
    "@angular-devkit/core": "~7.3.8",
    "@angular-devkit/schematics": "~7.3.8",
    "@angular/cli": "~7.3.8",
    "@angular/compiler": "~7.2.2",
    "@angular/compiler-cli": "~7.2.2",
    "@angular/language-service": "~7.2.2",
    "@ionic/angular-toolkit": "~1.5.1",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~12.0.0",
    "codelyzer": "~4.5.0",
    "cordova-plugin-device": "^2.0.2",
    "cordova-plugin-ionic-keyboard": "^2.1.3",
    "cordova-plugin-ionic-webview": "^4.0.1",
    "cordova-plugin-splashscreen": "^5.0.2",
    "cordova-plugin-statusbar": "^2.4.2",
    "cordova-plugin-whitelist": "^1.3.3",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.4.0",
    "ts-node": "~8.1.0",
    "tslint": "~5.16.0",
    "typescript": "~3.1.6",
    "worker-plugin": "^3.1.0"
  },
  "description": "An Ionic project",
  "cordova": {
    "plugins": {
      "cordova-plugin-whitelist": {},
      "cordova-plugin-statusbar": {},
      "cordova-plugin-device": {},
      "cordova-plugin-splashscreen": {},
      "cordova-plugin-ionic-webview": {
        "ANDROID_SUPPORT_ANNOTATIONS_VERSION": "27.+"
      },
      "cordova-plugin-ionic-keyboard": {},
      "cordova-plugin-qrscanner": {},
      "cordova-clipboard": {},
      "cordova-plugin-network-information": {}
    },
    "platforms": [
      "android"
    ]
  }
}